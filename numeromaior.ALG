algoritmo "semnome"
var
   numero1, numero2: inteiro
inicio
   escreva("digite um numero: ")
   leia(numero1)
   escreva("digite outro numero: ")
   leia(numero2)

   se (numero1 > numero2) entao
      escreva("o numero maior na equacao é: ", numero1)
   senao
      escreva("o numero maior na equacao é: ", numero2)
   fimse
fimalgoritmo