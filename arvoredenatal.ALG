algoritmo "semnome"
var
  x, y: inteiro
inicio

   para x de 1 ate 5 faca
        para y de 1 ate x faca
             escreva("*")
             se (y = 3) e (x = 3) entao
                escreva(" Feliz Natal")
             fimse
        fimpara
        escreval("")
   fimpara
   
   para x de 1 ate 3 faca
        escreval("*")
   fimpara
  
fimalgoritmo